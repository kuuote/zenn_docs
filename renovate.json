{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "timezone": "Asia/Tokyo",
  "dependencyDashboard": false,
  "automerge": true,
  "branchConcurrentLimit": 0,
  "packageRules": [
    {
      "matchFileNames": [
        "package.json"
      ],
      "groupName": "all package.json dependencies."
    }
  ]
}
